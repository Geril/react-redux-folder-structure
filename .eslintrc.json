{
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "indent": ["error", 4, {
            "SwitchCase": 1
        }],
        "max-len": "off",
        "react/jsx-indent": [2, 4]
    },
    "settings": {
        "import/resolver": "webpack"
    }
}